extends layout

block content
	// Contact Begin
section#contact.section.section--contact.p-t-0
	.container-fluid
		.row
			.col-xs-12.col-md-11.col-md-centered.col-lg-10.col-lg-push-1
				.heading
					h1.heading__title Contact
					h6.heading__subtitle
						| feel free to contact with me I would love to hear from you
				.row
					.col-sm-12
						form#contact-form.form(method='post', action='')
							input(type='text', name='foo', style='display:none;')
							.form__control
								input#name.form__input(type='text', name='name', placeholder='Your name')
								label.form__label(for='name')
									svg.form__icon(xmlns='http://www.w3.org/2000/svg', width='20', height='20', viewbox='0 0 20 20')
										g(fill='#FFF')
											path(d='M8.518 11.45h.146c-.04-.032-.08-.065-.114-.1-1.157-1.043-2.038-3.074-2.038-4.714 0-2.322 1.9-3.848 3.697-3.848 1.795 0 3.6 1.65 3.6 3.972 0 1.64-.976 3.608-2.187 4.616.248.947 1.607 1.257 4.41 2.025 3.113.854 2.575 2.97 2.566 4.388H1.8c-.01-1.418-.78-3.534 2.332-4.387 2.725-.75 4.082-1.062 4.386-1.95z')
											path(d='M8.663 11.45h-.145l.03-.1c.035.038.075.068.115.1z')
							.form__control
								input#email.form__input(type='text', name='email', placeholder='Your email')
								label.form__label(for='email')
									svg.form__icon(xmlns='http://www.w3.org/2000/svg', width='20', height='20', viewbox='0 0 20 20')
										g(fill='#FFF')
											path(d='M10.267 12.27c-.093.094-.22.145-.352.145s-.26-.055-.355-.145L3.048 5.755C3.028 5.835 3 5.912 3 6v8c0 .552.447 1 1 1h12c.554 0 1-.447 1-1V6c0-.138-.023-.27-.076-.387l-6.657 6.657z')
											path(d='M9.915 11.208l6.187-6.188c-.038-.003-.065-.02-.1-.02H4c-.086 0-.164.03-.245.048l6.16 6.16z')
							.form__control
								textarea#message.form__textarea(name='message', placeholder='Your message')
								label.form__label(for='message')
									svg.form__icon(xmlns='http://www.w3.org/2000/svg', width='20', height='20', viewbox='0 0 20 20')
										g(fill='#FFF')
											path(d='M2.99 17l4.18-1.124-3.06-3.057M15.043 8.02l-7.497 7.498-3.073-3.072 7.497-7.5zM15.653 3.46c-.61-.613-1.57-.613-2.182 0L12.33 4.6l3.08 3.08 1.143-1.142c.61-.61.61-1.57 0-2.182l-.897-.897z')
							.row.row-xs-center
								.col-xs-7.col-md-9
									small.form__error
								.col-xs-5.col-md-3
									button.form__submit.pull-xs-right(type='submit') Send
					.col-sm-12
						nav#nav
							a.icon.active.fa.fa-linkedin(target='_blank', href='https://www.linkedin.com/in/abubakr-mahmoud-074956106/')
								span linkedin
							a.icon.fa.fa-facebook(target='_blank', href='https://www.facebook.com/bakr.mahmoud.927')
								span facebook
							a.icon.fa.fa-twitter(target='_blank', href='https://twitter.com/abubakr28355335')
								span twitter
							a.icon.fa.fa-stack-overflow(target='_blank', href='https://stackoverflow.com/users/6847357/abu-bakr')
								span stackover
							a.fa.fa-codepen(target='_blank', href='https://codepen.io/abubakr/')
								span codepen
							a.icon.fa.fa-skype(href='live:abubakr8911')
								span skype
			// col
		// row
	// container-fluid
// Contact End
// content
